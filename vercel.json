{
  "builds": [
    {
      "src": "proxy.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "bg.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/proxy", "dest": "/proxy.py" },
    { "src": "/bg.png", "dest": "/bg.png" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
