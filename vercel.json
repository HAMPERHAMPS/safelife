{
  "builds": [
    {
      "src": "favicon.png",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "bg.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/favicon.png", "dest": "/favicon.png" },
    { "src": "/bg.png", "dest": "/bg.png" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
